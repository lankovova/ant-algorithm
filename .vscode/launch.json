{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug",
			"type": "chrome",
			"request": "launch",
			"webRoot": "${workspaceRoot}",
			"url": "http://localhost:8080",
			"sourceMaps": true,
			"smartStep": true,
			"internalConsoleOptions": "openOnSessionStart",
			"skipFiles": [
				"node_modules/**"
			],
			"sourceMapPathOverrides": {
				"webpack:///*": "${webRoot}/*"
			}
		}
	]
}
